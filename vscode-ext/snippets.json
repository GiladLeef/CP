{
	"Main function": {
		"prefix": "main",
		"body": [
			"int main($1){",
			"   $2",
            "   return 0;",
            "}"
		],
		"description": "Log output to console"
	},
    "function": {
        "prefix": "function",
        "body": [
            "$1 $2($3){",
            "   $4",
            "}"
        ]
    },
    "class": {
        "prefix": "class",
        "body": [
            "class $1 {",
            "   $2",
            "}"
        ]
    },
    "variable": {
        "prefix": "var",
        "body": [
            "$1 $2 = $3;"
        ]
    },
    "if": {
        "prefix": "if",
        "body": [
            "if ($1){",
            "   $2",
            "}"
        ]
    },
    "while": {
        "prefix": "while",
        "body": [
            "while ($1){",
            "   $2",
            "}"
        ]
    },
    "do while": {
        "prefix": "do",
        "body": [
            "do {",
            "   $1",
            "} while ($2);"
        ]
    },
    "for loop": {
        "prefix": "for",
        "body": [
            "for ($1; $2; $3) {",
            "   $4",
            "}"
        ]
    }
}